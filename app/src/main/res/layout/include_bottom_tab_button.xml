<?xml version="1.0" encoding="utf-8"?><!-- BottomTab Bar Button Layout-->
<FrameLayout android:layout_width="match_parent"
    android:id="@+id/frameBottomBar"
    android:layout_height="wrap_content"
    android:layout_alignParentBottom="true"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        >

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/grayFont" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@color/white">

            <ImageButton
                android:id="@+id/homeBtn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_home"
                android:scaleType="fitCenter"
                android:layout_weight="1" />

            <ImageButton
                android:id="@+id/searchBtn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_search"
                android:scaleType="fitCenter"
                android:layout_weight="1" />

            <ImageButton
                android:id="@+id/shoppingListBtn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_shopping"
                android:scaleType="fitCenter"
                android:layout_weight="1" />

            <ImageButton
                android:id="@+id/chattingBtn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_chatting"
                android:scaleType="fitCenter"
                android:layout_weight="1" />

            <ImageButton
                android:id="@+id/mypageBtn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/selector_my"
                android:scaleType="fitCenter"
                android:layout_weight="1" />
        </LinearLayout>
    </LinearLayout>
</FrameLayout>